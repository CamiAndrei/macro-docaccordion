  <artifactId>macro-docaccordion-ui</artifactId>
  <name>Document Accordion Macro - UI</name>
  <packaging>xar</packaging>
  <description>Provides a UI page with some js code used by the Document Accordion Macro</description>
  <properties>
    <!-- Don't run CLIRR here since there's no Java code. -->
    <xwiki.clirr.skip>true</xwiki.clirr.skip>
    <!-- Name to display by the Extension Manager -->
